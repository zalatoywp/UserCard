<style type="text/css">
*, *:before, *:after {
  box-sizing: border-box;
}

html, body {
  margin-top: 20px;
  height: auto;
  background-color: #24292e;
}

body {
  padding: 0;
  margin: 0;
  background: #01AAEC ;
  background: #e6ecf0 ;
  display: -webkit-box;
  display: -moz-box;
  display: box;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -moz-box-pack: center;
  box-pack: center;
  -webkit-justify-content: center;
  -moz-justify-content: center;
  -ms-justify-content: center;
  -o-justify-content: center;
  justify-content: center;
  -ms-flex-pack: center;
  -webkit-box-align: center;
  -moz-box-align: center;
  box-align: center;
  -webkit-align-items: center;
  -moz-align-items: center;
  -ms-align-items: center;
  -o-align-items: center;
  align-items: center;
  -ms-flex-align: center;
  font-family: "Source Sans Pro", sans-serif;
  
}

.profile {
  width: 360px;
  overflow: hidden;
  border-radius: 40px;
  box-shadow: 0px 0px 20px 6px rgba(42, 213, 243, 0.418), 0px 0px 20px 5px #01aaec3b
}
.profile section:nth-of-type(1) {
  position: relative;
}
.profile section:nth-of-type(1):after {
  content: "";
  width: 100%;
  height: 120px;
  background-color: transparent;
  background-image: -webkit-linear-gradient(top, transparent, black);
  background-image: linear-gradient(to bottom,transparent, black);
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 1;
}
.profile section:nth-of-type(1) img {
  width: 100%;
  height: auto;
  border-radius: 99%;
}
.profile section:nth-of-type(1) .text {
  font-family: 'Open Sans';
  position: absolute;
  text-decoration: none;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 15px 10px;
  text-align: center;
  z-index: 2;
  box-shadow: 0px 20px 20px 0px hsla(186, 100%, 2%, 0.418), 0px 20px 20px 0px #00000033
}
.profile section:nth-of-type(1) .text p {
  color: #ffffff;
  text-decoration: none;
  font-size: 26px;
  font-weight: 400;
  letter-spacing: 1px;
  margin-top: 0;
  margin-bottom: 5px;
  text-shadow: 0 0 8px #000000;
}
.profile section:nth-of-type(1) .text a {
  color: #ffffff;
  text-decoration: none;
  font-size: 16px;
  font-weight: 300;
  letter-spacing: 1px;
  text-shadow: 0 0 8px #000000;
}
.profile section:nth-of-type(2) {
  width: 100%;
  background: #fff;
  padding: 50px 20px 25px;
  padding-top: 10px;
  position: relative;
}
.profile section:nth-of-type(2) .info {
  margin-bottom: 20px;
  text-decoration: none;
  overflow: hidden;
  position: relative;
  z-index: 1;
}
.profile section:nth-of-type(2) .info div {
  float: left;
  width: 25%;
  text-align: center;
  
}
.profile section:nth-of-type(2) .info div span {
  display: block;
}
.profile section:nth-of-type(2) .info div span:nth-of-type(3) {
  color: #4d4d4d;
  font-size: 22px;
  font-weight: 800;
  
}
.profile section:nth-of-type(2) .info div span:nth-of-type(2) {
  color: #4099FF;
  font-size: 16px;
  font-weight: 800;
 
}
.profile section:nth-of-type(2) .info div span:nth-of-type(1) {
  color: #8f0303;
  font-size: 16px;
  font-weight: 800;
  padding: 2px;
  
}
.profile section:nth-of-type(2) .button {
  text-align: center;
  width: 100%;
  position: relative;
  z-index: 1;
}
.profile section:nth-of-type(2) .button a {
  display: inline-block;
  background: #4099FF;
  color: #fff;
  font-size: 22px;
  font-weight: 400;
  text-decoration: none;
  padding: 15px 50px 15px 90px;
  position: relative;
  overflow: hidden;
  border-radius: 30px;
  -webkit-transition: background 0.4s ease-in-out;
  -moz-transition: background 0.4s ease-in-out;
  transition: background 0.4s ease-in-out;
}
.profile section:nth-of-type(2) .button a:hover {
  background: #0d7eff;
}
.profile section:nth-of-type(2) .button a svg {
  position: absolute;
  top: 10px;
  left: 25px;
  width: 40px;
  height: 40px;
}
.profile section:nth-of-type(2) .button a svg path {
  fill: #fff;
}
.profile section:nth-of-type(2) > svg {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 200%;
  height: 200%;
  z-index: 0;
  margin-top: -60%;
  margin-left: -100%;
}
.profile section:nth-of-type(2) > svg path {
  fill: #f7f7f7;
}

footer {
  position: fixed;
  right: 30px;
  bottom: 40px;
}
footer a {
  color: #fff;
  font-size: 17px;
  letter-spacing: 2px;
  text-decoration: none;
  font-weight: 300;
  display: inline-block;
  border-bottom: 2px dotted #fff;
  -webkit-transition: border-color 0.4s ease-in-out;
  -moz-transition: border-color 0.4s ease-in-out;
  transition: border-color 0.4s ease-in-out;
}
footer a:hover {
  border-color: #0d7eff;
}


body {
  background-color: #24292e !important;
  color: white;
}

</style>
  
</head>

